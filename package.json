{
  "name": "visual-bookmarks-dev",
  "version": "1.0.0",
  "description": "Visual bookmarks",
  "scripts": {
    "lint:es": "eslint src/js/**",
    "lint:css": "stylelint src/css/**;  exit 0",
    "lint:html": "htmlhint src/*.html; exit 0",
    "lint": "npm run lint:es && npm run lint:css && npm run lint:html",
    "start": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "zip": "bestzip extension.zip extension/*",
    "release": "npm run build && npm run zip"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:k-ivan/visual-bookmarks-chrome.git"
  },
  "browserslist": [
    "last 10 Chrome versions"
  ],
  "author": "ivan_k",
  "license": "ISC",
  "homepage": "https://github.com/k-ivan/visual-bookmarks-chrome#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.10",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "bestzip": "^2.1.7",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.1",
    "eslint": "^6.8.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "htmlhint": "^0.11.0",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss": "^8.2.8",
    "postcss-csso": "^5.0.0",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.1.0",
    "postcss-nesting": "^7.0.1",
    "postcss-sort-media-queries": "^3.6.5",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "glory-modal": "^2.2.0",
    "sortablejs": "1.8.4",
    "tabs-slider": "^3.0.1"
  }
}
