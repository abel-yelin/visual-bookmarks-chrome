{
  "name": "visual-bookmarks-dev",
  "version": "1.0.0",
  "description": "Visual bookmarks",
  "scripts": {
    "lint:es": "eslint src/js/**",
    "lint:css": "stylelint src/css/**;  exit 0",
    "lint:html": "htmlhint src/*.html; exit 0",
    "lint": "npm run lint:es && npm run lint:css && npm run lint:html",
    "start": "webpack --mode=production --watch --progress --hide-modules",
    "build": "webpack --mode=production --progress --hide-modules",
    "zip": "bestzip extension.zip extension/*",
    "release": "npm run build && npm run zip"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:k-ivan/visual-bookmarks-chrome.git"
  },
  "browserslist": [
    "last 15 Chrome versions"
  ],
  "author": "ivan_k",
  "license": "ISC",
  "homepage": "https://github.com/k-ivan/visual-bookmarks-chrome#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "autoprefixer": "^9.6.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "bestzip": "^2.1.5",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^6.5.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "htmlhint": "^0.11.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-csso": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nesting": "^7.0.1",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "terser-webpack-plugin": "^2.1.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "glory-modal": "^1.2.2",
    "sortablejs": "1.8.1",
    "tabs-slider": "^1.2.2"
  }
}
